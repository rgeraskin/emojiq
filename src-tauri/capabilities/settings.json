{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "settings",
  "description": "Capability for the settings window",
  "windows": [
    "settings"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "shell:allow-open",
    "dialog:allow-ask"
  ],
  "local": true
}

